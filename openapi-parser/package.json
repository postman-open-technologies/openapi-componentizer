{
  "name": "openapi-componentizer",
  "version": "0.0.1",
  "description": "",
  "main": "build/index.js",
  "types": "build/index.d.ts",
  "scripts": {
    "dev": "tsup --watch",
    "clean": "rimraf build",
    "build:ts": "tsup src/index.ts --dts",
    "build": "npm-run-all clean build:ts",
    "start": "node build/cli/main.js",
    "format": "prettier --write \"src/**/*.{ts,tsx}\"",
    "prepare": "cd .. && husky install openapi-parser/.husky",
    "pre-commit": "lint-staged",
    "test": "jest"
  },
  "dependencies": {
    "cac": "^6.7.3",
    "js-yaml": "^4.1.0"
  },
  "devDependencies": {
    "@trivago/prettier-plugin-sort-imports": "^4.1.1",
    "@types/js-yaml": "^4.0.5",
    "@types/node": "^18.11.9",
    "@types/node-fetch": "^2.6.3",
    "husky": "^7.0.4",
    "lint-staged": "^13.0.3",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.5.1",
    "rimraf": "^3.0.2",
    "ts-node": "^10.4.0",
    "tsup": "^6.5.0",
    "typescript": "^4.5.4",
    "@types/jest": "^29.4.0",
    "jest": "^29.5.0",
    "ts-jest": "^29.0.5",
    "typedoc": "^0.23.28",
    "typedoc-plugin-markdown": "^3.14.0"
  },
  "lint-staged": {
    "*.{ts,tsx}": [
      "prettier --write"
    ]
  }
}